# WellSwap Platform Environment Variables
# 복사하여 .env.local로 저장하고 실제 값으로 변경하세요

# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Google OAuth 설정
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Hugging Face AI 설정
NEXT_PUBLIC_HUGGINGFACE_TOKEN=your_huggingface_token_here

# Google Cloud Vision API (선택사항)
GOOGLE_CLOUD_VISION_API_KEY=your_google_vision_api_key_here

# Azure Vision API (선택사항)
AZURE_VISION_ENDPOINT=your_azure_vision_endpoint_here
AZURE_VISION_API_KEY=your_azure_vision_api_key_here

# Resend Email API (선택사항)
RESEND_API_KEY=your_resend_api_key_here

# 관리자 지갑 주소
ADMIN_WALLET_ADDRESS=your_admin_wallet_address_here

# AI 서버 설정
NEXT_PUBLIC_AI_SERVER_URL=https://wellswaphk.onrender.com

# 블록체인 설정
NEXT_PUBLIC_CONTRACT_ADDRESS=your_contract_address_here
NEXT_PUBLIC_NETWORK_ID=97
